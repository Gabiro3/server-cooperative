{
    "version": 2,
    "name": "Blinkit",
    "builds": [
      {
        "src": "src/index.ts",   
        "use": "@vercel/node",   
        "config": {
          "maxLambdaSize": "50mb" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"  
      }
    ]
  }
  